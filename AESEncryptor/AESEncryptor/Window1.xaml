<?xml version="1.0" encoding="utf-8"?>
<Window
	x:Class="AESEncryptor.Window1" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Title="AESEncryptor"
	Height="555"
	Width="555">
	<Grid>
		<TextBox
			x:Name="CipherKey"
			Height="33.000000000000014"
			Grid.Column="0"
			Grid.Row="0"
			HorizontalAlignment="Stretch"
			VerticalAlignment="Top"
			Margin="10,10,222,0"
			Text="AnyTextButMustHave32charssssssss" />
		<Button
			x:Name="EncryptButton"
			Height="33"
			Content="Encrypt"
			Margin="341,10,118,0"
			VerticalAlignment="Top"
			HorizontalAlignment="Stretch"
			Grid.Row="0"
			Grid.Column="0"
			Click="Encrypt" />
		<TextBox
			x:Name="MyText"
			Grid.Column="0"
			Grid.Row="0"
			HorizontalAlignment="Stretch"
			VerticalAlignment="Stretch"
			TextWrapping="Wrap"
			Margin="10,50,10,10"
			Text="Put your text" />
		<Button
			Grid.Column="0"
			Grid.Row="0"
			HorizontalAlignment="Stretch"
			VerticalAlignment="Top"
			Margin="433,10,10,0"
			Height="33"
			x:Name="DecryptButton"
			Content="Decrypt"
			Click="Decrypt" />
	</Grid>
</Window>